      <div class="d-flex flex-column justify-content-center align-items-center">
        <h1 class="font-weight-bold text-dark text-lg-center solid_line">My Events</h1> 
        <%# Shows events I have created %>
        <ul class="list">
        <% for(var i=0; i<events.length; i++) {%>
          <li class="item">
            <a href="/event/<%= events[i]._id%>" class="headline"> <%= events[i].title%></a>
            <p class="font-weight-bold text-dark text-lg justify-content-start"><strong><%= helper.dateFormatter(events[i].eventDate) %></strong></p>
          </li>
        <% } %>
        </ul>
        <a class="btn btn-success justify-content-center button" href="/event/createEventPage">Create new event</a>
      </div> 
<%- include('partials/footer') -%>
